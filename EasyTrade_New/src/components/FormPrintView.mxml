<mx:VBox xmlns:fx="http://ns.adobe.com/mxml/2009" 
		  xmlns:s="library://ns.adobe.com/flex/spark" 
		  xmlns:mx="library://ns.adobe.com/flex/mx" width="100%" height="100%"
		  xmlns:components="components.*" paddingTop="50" paddingLeft="50" paddingRight="50"
		  >
	<!-- The template for the printed page, 
	with the contents for all pages. -->
	<mx:VBox width="20%" horizontalAlign="left">
		<mx:Label text="Event Log"/>
	</mx:VBox>
	<mx:PrintDataGrid id="myDataGrid" fontFamily="Verdana" width="100%" height="100%" >
		<mx:columns>
			<mx:DataGridColumn  dataField="CLIENT_CODE" />
			<mx:DataGridColumn  dataField="SYMBOL" />
			<mx:DataGridColumn  dataField="PUBLIC_ORDER_STATE" />
			<mx:DataGridColumn  dataField="PRICE" />
			<mx:DataGridColumn  dataField="FILLED" />
			<mx:DataGridColumn  dataField="VOLUME" />
			<mx:DataGridColumn  dataField="DISCLOSED_VOLUME" />
			<mx:DataGridColumn  dataField="ENTRY_DATETIME" />
			<mx:DataGridColumn  dataField="SIDE" />
			<mx:DataGridColumn  dataField="ORDER_TYPE" />
			<mx:DataGridColumn  dataField="ORDER_NO" />
			<mx:DataGridColumn  dataField="TICKET_ID" />
		</mx:columns>
	</mx:PrintDataGrid>
</mx:VBox>





